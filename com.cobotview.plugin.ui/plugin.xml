<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.cobotview.plugin.ui.perspective.CobotViewPerspectiveFactory"
            id="com.cobotview.ui.perspective"
            name="CobotView">
         <description>
            Cobot View
         </description>
      </perspective>
   </extension>
   <extension
      point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
           targetID="com.cobotview.ui.perspective">
         <view
               closeable="true"
               id="com.cobotview.plugin.ui.projectView"
               minimized="false"
               moveable="true"
               ratio="0.25"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               visible="true">
         </view>
         <hiddenMenuItem
               id="org.eclipse.ui.run">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="file">
         </hiddenMenuItem>
          <hiddenMenuItem
               id="help">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="org.eclipse.search.menu">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="project">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="navigate">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="refactorMenuId">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="sourceMenuId">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="edit">
         </hiddenMenuItem>
         <hiddenMenuItem
               id="window">
         </hiddenMenuItem>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?before=additions">
         <menu
               id="CobotFile"
               label="文件"
               mnemonic="F">
            <command
                  commandId="com.cobotview.plugin.ui.createProject"
                  icon="icons/menus/new_project.png"
                  label="新建"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeWorkbenchWindow.activePerspective">
                  <equals
                        value="com.cobotview.ui.perspective">
                  </equals>
               </with>
            </visibleWhen>
            <command
                  commandId="null"
                  label="打开C/C++"
                  style="push">
            </command>
            <command
                  commandId="com.cobotview.plugin.ui.openbinary"
                  label="打开二进制(exe或dll)"
                  style="push">
            </command>
            <command
                  commandId="com.cobotview.plugin.ui.exist"
                  label="退出"
                  style="push">
            </command>
         </menu>
         <menu
               id="CobotSearch"
               label="搜索"
               mnemonic="S">
            <command
                  commandId="com.cobotview.plugin.ui.searchText"
                  label="搜索文本"
                  style="push">
            </command>
            <command
                  commandId="com.cobotview.plugin.ui.searchC"
                  label="搜索C/C++"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeWorkbenchWindow.activePerspective">
                  <equals
                        value="com.cobotview.ui.perspective">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
         <menu
               id="CobotHelp"
               label="帮助"
               mnemonic="H">
            <command
                  commandId="org.eclipse.ui.help.aboutAction"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeWorkbenchWindow.activePerspective">
                  <equals
                        value="com.cobotview.ui.perspective">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?before=additions">
         <toolbar
               id="com.cobotview.plugin.ui.toolbar1">
            <command
                  commandId="com.cobotview.plugin.ui.newProject"
                  icon="icons/toolbar/new_project.png"
                  label="新建"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeWorkbenchWindow.activePerspective">
                     <equals
                           value="com.cobotview.ui.perspective">
                     </equals>
                  </with>
               </visibleWhen>
            </command>
         </toolbar>
         <toolbar
               id="com.cobotview.plugin.ui.toolbar2">
            <command
                  commandId="com.cobotview.openFile"
                  icon="icons/toolbar/open_file.gif"
                  label="打开"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeWorkbenchWindow.activePerspective">
                     <equals
                           value="com.cobotview.ui.perspective">
                     </equals>
                  </with>
               </visibleWhen>
            </command>
         </toolbar>
         <toolbar
               id="com.cobotview.plugin.ui.toolbar3">
            <command
                  commandId="com.cobotview.plugin.ui.search"
                  icon="icons/menus/search.png"
                  label="搜索"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeWorkbenchWindow.activePerspective">
                     <equals
                           value="com.cobotview.ui.perspective">
                     </equals>
                  </with>
               </visibleWhen>
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.cobotview.plugin.ui.cobotviewNavigator?after=additions">
         <command
               commandId="com.cobotview.plugin.ui.createProject"
               icon="icons/menus/new_project.png"
               label="新建二进制项目"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <not>
         false         <or>
                     <instanceof
                           value="org.eclipse.core.resources.IResource">
                     </instanceof>
                  </or>
               </not>
            </visibleWhen>
         </command>
         <command
               commandId="com.cobotview.plugin.ui.openCASMFile"
               style="push">
         </command>
         <command
               commandId="com.cobotview.plugin.ui.openbinary"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.cobotview.plugin.ui.commands.NewProjectToolBarHandler"
            id="com.cobotview.plugin.ui.newProject"
            name="新建">
         <defaultHandler
               class="com.cobotview.plugin.ui.perspective.NewProjectToolBarHandler">
         </defaultHandler>
      </command>
      <command
            id="com.cobotview.plugin.ui.openCASMFile"
            name="打开C/C++和asm">
         <defaultHandler
               class="com.cobotview.plugin.ui.commands.OpenCASMFileHandler">
         </defaultHandler>
      </command>
      <command
            id="com.cobotview.plugin.ui.openbinary"
            name="打开二进制(exe或dll)">
         <defaultHandler
               class="com.cobotview.plugin.ui.OpenBinaryHandler">
         </defaultHandler>
      </command>
      <command
            id="com.cobotview.plugin.ui.searchText"
            name="搜索文本">
         <defaultHandler
               class="com.cobotview.plugin.ui.commands.SearchTextHandler">
         </defaultHandler>
      </command>
      <command
            defaultHandler="com.cobotview.plugin.ui.commands.SearchCHandler"
            id="com.cobotview.plugin.ui.searchC"
            name="搜索C/C++">
      </command>
      <command
            id="com.cobotview.plugin.ui.createProject"
            name="新建二进制项目">
         <defaultHandler
               class="com.cobotview.plugin.ui.commands.NewProjectHandler">
         </defaultHandler>
      </command>
      <command
            id="com.cobotview.plugin.ui.exist"
            name="退出">
         <defaultHandler
               class="com.cobotview.plugin.ui.commands.Exist">
         </defaultHandler>
      </command>
   </extension>
      <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="com.cobotview.plugin.ui.wizards.category.cobotView"
            name="CobotView">
      </category>
      <wizard
            canFinishEarly="false"
            category="com.cobotview.plugin.ui.wizards.category.cobotView"
            class="com.cobotview.plugin.ui.wizards.CobotViewNewWizard"
            finalPerspective="com.cobotview.ui.perspective"
            hasPages="true"
            icon="icons/wizards/new_project.png"
            id="com.cobotview.plugin.ui.CobotViewNewWizard"
            name="CobotView Project"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.cobotview.plugin.ui.views.FunctionAddressView"
            icon="icons/views/functionAddressView.png"
            id="com.cobotview.plugin.ui.functionAddressView"
            name="函数地址映射图"
            restorable="true">
      </view>
      <view
            class="com.cobotview.plugin.ui.views.ContentView"
            id="com.cobotview.plugin.ui.contentView"
            name="ContentView"
            restorable="true">
      </view>
      <view
            class="com.cobotview.plugin.ui.views.GraphView"
            icon="icons/views/fileRelationGraph.gif"
            id="com.cobotview.plugin.ui.graphView"
            name="GraphView"
            restorable="true">
      </view>
      <view
            class="com.cobotview.plugin.ui.views.OutlineView"
            icon="icons/views/outline_co.png"
            id="com.cobotview.plugin.ui.outline"
            name="Outline"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            class="com.cobotview.plugin.ui.navigator.CobotViewNavigator"
            icon="icons/views/resource_persp.gif"
            id="com.cobotview.plugin.ui.cobotviewNavigator"
            name="项目资源管理器"
            restorable="true">
      </view>
   </extension>
      <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="com.cobotview.plugin.ui.cobotviewNavigator">
      </viewer>
      <viewerContentBinding
            viewerId="com.cobotview.plugin.ui.cobotviewNavigator">
         <includes>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="com.cobotview.plugin.ui.NavigatorFilter">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="com.cobotview.plugin.ui.cobotviewNavigator">
         <excludes>
            <actionExtension
                  pattern="org.springframework.ide.eclipse.ui.tools">
            </actionExtension>
         </excludes>
      </viewerActionBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonFilter
            activeByDefault="true"
            class="com.cobotview.plugin.ui.navigator.NavigatorFilter"
            id="com.cobotview.plugin.ui.NavigatorFilter"
            name="NavigatorFilter">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="com.cobotview.ui.perspective">
         <hiddenToolBarItem
               id="org.eclipse.search.OpenSearchDialogPage">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="newWizardDropDown">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.externaltools.ExternalToolMenuDelegateToolbar">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.mylyn.tasks.ui.command.openTask">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.mylyn.tasks.ui.navigation">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.file.saveAll">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.file.save">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.edit.text.gotoLastEditPosition">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.edit.text.actionSet.navigation">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="SearchField">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.navigate.next">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.navigate.previous">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.eclipse.ui.help.quickStartAction">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="openexplorer.actions.OpenExplorer">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id=" org.springsource.ide.eclipse.commons.launch.relaunch.action">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.springsource.ide.eclipse.commons.launch.stop.action">
         </hiddenToolBarItem>
         <hiddenToolBarItem
               id="org.springframework.ide.eclipse.beans.ui.commands.openBean">
         </hiddenToolBarItem>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.activities">
      <activity
            id="com.cobotview.plugin.ui.hidden"
            name="unwanted">
      </activity>
      <activityPatternBinding
            activityId="com.cobotview.plugin.ui.hidden"
            pattern="org.eclipse.jdt.ui.*">
      </activityPatternBinding>
      <activityPatternBinding
            activityId="com.cobotview.plugin.ui.hidden"
            pattern="org.eclipse.debug.ui.*">
      </activityPatternBinding>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.cobotview.plugin.ui.core.CobotViewStartUp">
      </startup>
   </extension>
</plugin>
